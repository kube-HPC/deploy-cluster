
---all---
kubernetes.namespace_name:default AND 
_exists_:message AND
_exists_:meta.type AND NOT
message:statistics

---catalog---
kubernetes.namespace_name:default AND 
_exists_:message AND
_exists_:meta.type AND
_exists_:meta.internal.videoData.vid

---users---
kubernetes.namespace_name:default AND
_exists_:message AND
_exists_:meta.type AND
_exists_:meta.internal.usersCatalog.vid

---workers----
kubernetes.namespace_name:default AND
_exists_:message AND
_exists_:meta.type AND NOT
message:statistics AND
kubernetes.pod_name:worker*

---workers videos---
kubernetes.namespace_name:default AND
_exists_:message AND
_exists_:meta.type AND
_exists_:meta.internal.videoData.sensorID

---specific service---
kubernetes.namespace_name:default AND
_exists_:message AND
_exists_:meta.type AND
meta.type:worker

auth, signalling, supervisor, worker

---all statistics---
kubernetes.namespace_name:default AND
_exists_:message AND
_exists_:meta.type AND
message:statistics

---specific vid---
kubernetes.namespace_name:default AND
_exists_:message AND
_exists_:meta.type AND
_exists_:meta.internal.videoData.labels.status AND
meta.internal.videoData.sensorID:Zik1
